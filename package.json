{
  "name": "artell-artist-notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "@material-ui/core": "^4.11.3",
    "@mojotech/json-type-validation": "^3.1.0",
    "@stripe/stripe-js": "^1.13.1",
    "@svgr/webpack": "^5.5.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.5.0",
    "next": "^10.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-lazyload": "^3.2.0",
    "sass": "^1.32.8",
    "sharp": "^0.28.3",
    "stripe": "^8.138.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lintcheck": "eslint --ext tsx,ts --max-warnings 0 src",
    "formatcheck": "prettier -c src",
    "allcheck": "npm run typecheck && npm run lintcheck && npm run formatcheck",
    "format": "prettier src -w",
    "ts-run": "ts-node -O '{\"module\": \"CommonJS\"}' -r tsconfig-paths/register"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-explicit-any": "error",
      "@typescript-eslint/no-non-null-assertion": "off",
      "react/prop-types": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/jest": "26.0.21",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-lazyload": "^3.1.0",
    "@types/sharp": "^0.28.5",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "dotenv": "^9.0.2",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "tsconfig-paths": "^3.9.0",
    "vercel": "^21.3.3"
  }
}
